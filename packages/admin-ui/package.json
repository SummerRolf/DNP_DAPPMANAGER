    "express": "^4.17.1",
    "nodemon": "^2.0.4",
    "prettier": "^1.16.4",
    "why-did-you-update": "0.1.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "watchPathIgnorePatterns": [
      "/node_modules"
    ]
  }
